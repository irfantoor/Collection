{
    "name": "irfantoor/collection",
    "description": "Collection implementing ArrayAccess, Countable and IteratorAggregate",
    "keywords": ["collection", "array", "countable", "iterator", "readonly"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Irfan TOOR",
            "email": "email@irfantoor.com"
        }
    ],
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "irfantoor/test": "^0.6",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "IrfanTOOR\\": "src"
        }
    },
    "scripts": {
        "test": [
            "@ittest",
            "@phpcs"
        ],
        "ittest": "php vendor/bin/test tests",
        "phpcs": "php vendor/bin/phpcs src"
    }
}
