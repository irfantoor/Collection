{
    "name": "irfantoor/collection",
    "description": "Collection implementing ArrayAccess, Countable and IteratorAggregate",
    "keywords": ["collection","array","countable","iterator"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Irfan TOOR",
            "email": "email@irfantoor.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2",
        "irfantoor/test": "^0.1.1"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "IrfanTOOR\\": "src"
        }
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpcs"
        ],
        "phpunit": "php vendor/bin/phpunit tests",
        "phpcs": "php vendor/bin/phpcs src"
    }
}
